  "name": "@nextstrain/nextclade-web",
  "version": "3.15.0",
  "description": "Clade assignment, mutation calling, and sequence quality checks",
  "homepage": "https://clades.nextstrain.org",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nextstrain/nextclade.git"
  },
  "keywords": [
    "nextstrain",
    "nextclade",
    "phylogenetics",
    "clade",
    "mutations",
    "qc"
  ],
  "bugs": {
    "url": "https://github.com/nextstrain/nextclade/issues"
  },
  "license": "MIT",
  "browserslist": {
    "production": [
      "defaults",
      "not IE 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "nohoist": [
    "estree-walker",
    "estree-walker/**",
    "**/estree-walker",
    "**/estree-walker/**"
  ],
  "scripts": {
    "clear": "node -e \"process.stdout.write(process.platform === 'win32' ? '\\u001Bc' : '\\u001B[2J\\u001B[3J\\u001B[H')\"",
    "par": "bun x run-p -c --aggregate-output --color=always",
    "ser": "bun x run-s -c --color=always",
    "postinstall": "bun par monkey-patch",
    "monkey-patch": "bun run tools/monkeyPatch.ts",
    "next:dev": "NODE_ENV=development next dev --hostname 0.0.0.0 --port 3000",
    "next:build": "NODE_ENV=production next build",
    "next:prod:postbuild": "cp -avr .next/static/{app.json,robots.txt} .build/production/web/",
    "dev": "bun install && bun run ser clear postinstall clear next:dev",
    "prod:build": "bun install && bun run ser clear postinstall clear next:build next:prod:postbuild",
    "prod:serve": "bun x serve --cors --no-clipboard --listen=8080 .build/production/web",
    "prod": "bun install && bun run ser clear prod:build prod:serve",
    "lint": "bun par eslint tsc",
    "lint:fix": "bun par eslint:fix tsc",
    "lint:ci": "bun par eslint:ci tsc",
    "eslint": "eslint  --config eslint.config.mjs --format friendly '**/*.{js,mjs,cjs,jsx,ts,mts,cts,tsx}'",
    "eslint:fix": "bun eslint --config eslint.config.mjs --fix",
    "eslint:ci": "bun eslint --config eslint.config.mjs --max-warnings=0",
    "eslint:inspector": "node node_modules/.bin/eslint-config-inspector --config eslint.config.mjs",
    "tsc": "tsgo --project tsconfig.json --noEmit",
    "format": "prettier --check '**/*.{astro,js,mjs,cjs,jsx,ts,mts,cts,tsx,json,html,css,less,scss,vue,yml,yaml}'",
    "format:fix": "bun format --write",
    "wasm-dev": "bun ser wasm-dev:generate types:generate",
    "wasm-dev:generate": "cargo build -q --profile=opt-dev --package=nextclade-web --lib --target=wasm32-unknown-unknown --target-dir=.build/docker/wasm-dev/ && wasm-bindgen .build/docker/wasm-dev/wasm32-unknown-unknown/opt-dev/nextclade_web.wasm --target=bundler --out-dir=.build/wasm-prod/ --out-name=nextclade-wasm && mkdir -p src/gen/ && cp .build/wasm-prod/* src/gen/",
    "wasm-prod": "bun ser wasm-prod:generate types:generate",
    "wasm-prod:generate": "wasm-pack --quiet --log-level=error build --release --target=bundler --out-dir=.build/wasm-prod/ --out-name=nextclade-wasm -- --package=nextclade-web --locked && mkdir -p src/gen/ && cp .build/wasm-prod/* src/gen/",
    "types:clear": "rimraf src/gen/_SchemaRoot.d.ts",
    "types:generate": "bun run tools/generateTypes.ts -i src/gen/_SchemaRoot.json -o src/gen/_SchemaRoot.d.ts"
  },
